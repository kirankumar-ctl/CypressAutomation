{
    "chromeWebSecurity": false,
    "defaultCommandTimeout":70000,
    "experimentalFetchPolyfill": true,
    "testFiles": "**/*.feature",

    "reporter": "cypress-multi-reporters",
    "reporterOptions": {
        "reporterEnabled": "mochawesome",
        "mochawesomeReporterOptions": {
            "reportDir": "cypress/reports/mocha",
            "quite": true,
            "overwrite": false,
            "html": true,
            "json": true
        }
    }
}
